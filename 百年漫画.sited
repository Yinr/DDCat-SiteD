        /* fix host error of image url */
        var urlStart = imgurl.lastIndexOf("https://");
        if (urlStart > 0) {
            imgurl = imgurl.substr(urlStart);
        }

        return urla(imgurl, mainHost);
    });

    return JSON.stringify(list);
